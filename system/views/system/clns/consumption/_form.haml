%tbody
  %tr
    %td
      %table.w-100pr
        %tbody.inner
          %tr.consumption-freight-header.hidden
            - mat(@object,'table_header_form').each do |label|
              %td= label
          %tr.consumption-freight.template
            - o = @object.freights.new
            %td
              %span.name Material
              - input_for o, 'freight_id',    {type: 'hidden',nested: 'doc_con',index: 0,style: 'freight_id',value: 'strip'}
              - input_for o, 'id_date',       {type: 'hidden',nested: 'doc_con',index: 0,style: 'id_date',value: 'strip'}
              - input_for o, 'id_stats',      {type: 'hidden',nested: 'doc_con',index: 0,style: 'id_stats',value: 'strip'}
              - input_for o, 'id_intern',     {type: 'hidden',nested: 'doc_con',index: 0,style: 'id_intern',value: 'false'}
              - input_for o, 'um',            {type: 'hidden',nested: 'doc_con',index: 0,style: 'um',value: 'strip'}
              - input_for o, 'pu',            {type: 'hidden',nested: 'doc_con',index: 0,style: 'pu',value: 'strip'}
              - input_for o, 'qu',            {type: 'hidden',nested: 'doc_con',index: 0,style: 'qu',value: 'strip'}
              - input_for o, 'val',           {type: 'hidden',nested: 'doc_con',index: 0,style: 'val',value: 'strip'}
            %td.ta-ce.w-4rem
              %span.um buc
            %td.ta-ri.w-6rem
              %span.pu 0.00
            %td.ta-ri.w-8rem
              %span.stk.qu 0.00
            %td.ta-ri.w-8rem
              %span.out.qu 0.00
            %td.ta-ri.w-8rem
              %span.val 0.00
            %td.ta-ce.w-2rem
              %span.button.red.pointer.fa.fa-minus-circle
          %tr.consumption-freight-total.hidden
            %td{colspan: 5}
              %span.name TOTAL
            %td
              %span.val 0.00
            %td
              %span &nbsp
